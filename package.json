{
  "name": "@duckduckgo/autofill",
  "main": "dist/autofill.js",
  "browserslist": [
    "> 0.25%",
    "not dead",
    "not ie 11"
  ],
  "scripts": {
    "start": "grunt dev",
    "build": "grunt",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "copy-build-integration": "cp dist/autofill.js integration-test/extension && cp dist/autofill.css integration-test/extension/public/css/",
    "open-test-extension": "web-ext run -t chromium -u https://privacy-test-pages.glitch.me/ -s integration-test/extension",
    "test": "jest && npm run test:integration",
    "test:integration": "npm run copy-build-integration && jasmine --config=integration-test/config.js",
    "test:watch": "jest --watch"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/preset-env": "^7.13.15",
    "asana": "^0.18.6",
    "babelify": "^10.0.0",
    "eslint": "^7.25.0",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "grunt": "^1.4.0",
    "grunt-babel": "^8.0.0",
    "grunt-browserify": "^6.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-eslint": "^23.0.0",
    "grunt-exec": "^3.0.0",
    "jasmine": "^4.0.2",
    "jest": "^27.4.7",
    "jest-chrome": "^0.7.1",
    "load-grunt-tasks": "^5.1.0",
    "markdown-it": "^12.2.0",
    "puppeteer": "^13.0.1"
  }
}
