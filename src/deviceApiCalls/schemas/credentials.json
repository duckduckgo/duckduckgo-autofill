{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Credentials",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "id": {
      "description": "If present, must be a string",
      "type": "string"
    },
    "username": {
      "description": "This field is always present, but sometimes it could be an empty string",
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "origin": {
      "type": "object",
      "properties": {
        "url": {
          "type": "string"
        },
        "partialMatch": {
          "type": "boolean"
        }
      },
      "required": ["url", "partialMatch"],
      "additionalProperties": false
    },
    "credentialsProvider": {
      "type": "string",
      "enum": [
        "duckduckgo",
        "bitwarden"
      ]
    },
    "providerStatus": {
      "type": "string",
      "enum": [
        "locked",
        "unlocked"
      ]
    }
  },
  "required": [
    "username",
    "password"
  ]
}
