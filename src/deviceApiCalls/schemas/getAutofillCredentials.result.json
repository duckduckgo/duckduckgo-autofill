{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "GetAutofillCredentialsResult",
  "type": "object",
  "description": "This describes return values for `getAutofillCredentials`",
  "additionalProperties": false,
  "examples": [
    {
      "autogenerated": true,
      "password": "abcd9876543221",
      "username": ""
    },
    {
      "id": "1",
      "password": "123456",
      "username": "dax@example.com"
    }
  ],
  "properties": {
    "type": {
      "description": "Required on Android + Windows devices, optional on iOS/macos",
      "type": "string",
      "const": "getAutofillCredentialsResponse"
    },
    "success": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "string"
        },
        "autogenerated": {
          "description": "Whether or not this credential was autogenerated or not",
          "type": "boolean"
        },
        "username": {
          "description": "A username if one is available. Note: this field can be an empty string",
          "type": "string"
        },
        "password": {
          "type": "string"
        }
      },
      "required": ["username"]
    },
    "error": {
      "$ref": "./error.json"
    }
  }
}
