{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "GetAutofillDataFocusResponse",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "type": {
      "description": "Required on iOS, to show keyboard extension",
      "type": "string",
      "const": "getAutofillDataFocusResponse"
    },
    "success": {
      "description": "The data returned, containing only fields that will be auto-filled",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "creditCards": { "$ref": "./creditCard.json" },
        "action": {
          "type": "string",
          "enum": ["fill", "none"]
        }
      },
      "required": ["action"]
    },
    "error": {
      "$ref": "./error.json"
    }
  }
}
